{
  "manifest_version": 3,
  "name": "Bookmarks",
  "version": "1.0.0",
  "description": "New tab page",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs",
    "tabGroups",
    "activeTab",
    "scripting",
    "history",
    "contextMenus"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Bookmark",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["modal-styles.css"],
      "matches": ["<all_urls>"]
    }
  ]
}

